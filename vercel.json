{
  "version": 2,
  "builds": [
    {
      "src": "api/airtable.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/airtable",
      "dest": "/api/airtable.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard-formateur.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
